<odoo>
    <data>
        <record id ="import_drivin_action" model ="ir.ui.view">
            <field name ="name">Importar Drivin</field>
            <field name ="model">stock.piking.import.drivin</field>
            <field name ="arch" type = "xml">
                <form string="Importar Drivin">
                    <sheet>
                        
                        <group>
                            <field name="update_date"/>
                            <field name="excel_file"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name ="action_stock_import_drivin" string="Importar" type ="object"/>
                        <button string="Cancelar" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>

            </field>
        </record>

        <act_window name="Importar Drivin"
            id="import_drivin_action"
            res_model="stock.piking.import.drivin"
            binding_model="stock.picking"
            binding_views="list"
            view_mode="form"
            target="new"
        />

        <record id ="import_drivin_account_action" model ="ir.ui.view">
            <field name ="name">Importar Drivin</field>
            <field name ="model">account.move.import.drivin</field>
            <field name ="arch" type = "xml">
                <form string="Importar Drivin">
                    <sheet>
                        
                        <group>
                            <field name="update_date"/>
                            <field name="excel_file"/>
                        </group>
                    </sheet>
                    <footer>
                        <button name ="action_account_import_drivin" string="Importar" type ="object"/>
                        <button string="Cancelar" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>

            </field>
        </record>

         <act_window name="Importar Drivin"
            id="import_drivin_account_action"
            res_model="account.move.import.drivin"
            binding_model="account.move"
            binding_views="list"
            view_mode="form"
            target="new"
        />
        
    </data>
</odoo>